<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="213dp"
        tools:layout_editor_absoluteY="-196dp">

        <Button
            android:id="@+id/button10"
            android:layout_width="289dp"
            android:layout_height="38dp"
            android:backgroundTint="#FF9800"
            android:foregroundTint="#FF9800"
            android:text="kundenlöschung"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button9" />

        <Button
            android:id="@+id/button9"
            android:layout_width="289dp"
            android:layout_height="38dp"
            android:backgroundTint="#FF9800"
            android:foregroundTint="#FF9800"
            android:text="Kunden Modifizieren"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button8" />

        <Button
            android:id="@+id/button8"
            android:layout_width="289dp"
            android:layout_height="38dp"
            android:backgroundTint="#FF9800"
            android:foregroundTint="#FF9800"
            android:text="Kundenerstellung"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button6" />

        <Button
            android:id="@+id/button6"
            android:layout_width="289dp"
            android:layout_height="38dp"
            android:backgroundTint="#FF9800"
            android:foregroundTint="#FF9800"
            android:text="Autolöschung"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button5" />

        <Button
            android:id="@+id/button5"
            android:layout_width="289dp"
            android:layout_height="38dp"
            android:backgroundTint="#FF9800"
            android:foregroundTint="#FF9800"
            android:text="auto modifizieren"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button4" />

        <Button
            android:id="@+id/button4"
            android:layout_width="289dp"
            android:layout_height="38dp"
            android:backgroundTint="#FF9800"
            android:foregroundTint="#FF9800"
            android:text="Autoerstellung"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button7" />

        <Button
            android:id="@+id/button7"
            android:layout_width="289dp"
            android:layout_height="38dp"
            android:backgroundTint="#FF9800"
            android:foregroundTint="#FF9800"
            android:text="Verwaltung"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.491"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button3" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="414dp"
            android:layout_height="194dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/mobile_de" />

        <TextView
            android:id="@+id/welcome"
            android:layout_width="292dp"
            android:layout_height="20dp"
            android:background="#FF9800"
            android:fontFamily="sans-serif-black"
            android:foregroundTint="#FFFFFF"
            android:text="WILKOMMEN BEI MOBILE.DE :)"
            android:textAlignment="center"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView" />

        <Button
            android:id="@+id/button1"
            android:layout_width="289dp"
            android:layout_height="38dp"
            android:backgroundTint="#FF9800"
            android:foregroundTint="#FF9800"
            android:text="Login"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/space" />

        <Button
            android:id="@+id/button2"
            android:layout_width="289dp"
            android:layout_height="38dp"
            android:backgroundTint="#FF9800"
            android:foregroundTint="#FF9800"
            android:text="Logout"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button1" />

        <Button
            android:id="@+id/button3"
            android:layout_width="289dp"
            android:layout_height="38dp"
            android:backgroundTint="#FF9800"
            android:foregroundTint="#FF9800"
            android:text="Autoauswahl"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button2" />

        <Space
            android:id="@+id/space"
            android:layout_width="131dp"
            android:layout_height="28dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/welcome" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>